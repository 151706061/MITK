<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.blueberry.osgi.applications">
    <application id="org.mitk.example.selectionserviceqt">
      <run class="SelectionServiceQT"/>
    </application>
  </extension>

  <extension point="org.blueberry.ui.perspectives">
    <perspective
      id="org.mitk.example.extendedperspective"
      name="Extended Example Perspective"
      icon="resources/perspectiveIcon1.png"
      class="ExtendedPerspective">
    </perspective>
  </extension>

<extension point="org.blueberry.ui.views">
    <view
      id="org.mitk.views.listenerview"
      name="Selection Listener Example"
      icon="resources/selectionListenerIcon.png"
      class="ListenerView">
    </view>
  </extension>

<extension point="org.blueberry.ui.views">
    <view
      id="org.mitk.views.selectionview"
      name="Selection Provider Example"
      icon="resources/selectionProviderIcon.png"
      class="SelectionView">
    </view>
  </extension>

</plugin>
