<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.blueberry.osgi.applications">
    <application id="org.mitk.example.extensionpointcontribution">
      <run class="ExtensionPointContribution"/>
    </application>
  </extension>
  
  <extension point="org.blueberry.ui.perspectives">
    <perspective
      id="org.mitk.example.minimalperspective"
      name="Minimal Example Perspective"
      class="MinimalPerspective">
    </perspective>
  </extension>
  
  <extension point="org.blueberry.ui.views">
    <view 
      id="org.mitk.views.minimalview" 
      name="Minimal View"
      class="MinimalView">
    </view>
  </extension>
  
</plugin>
