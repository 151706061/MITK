<?xml version="1.0" standalone=no>
<DICOMFileReader
  label="Slice Location Simple"
  description="Sort images by Slice Location, require equal distance between locations; simple grouping"
  class="DICOMITKSeriesGDCMReader"
  version="1"
  fixTiltByShearing="true"
  simpleVolumeImport="true">
<DICOMDatasetSorter class="DICOMTagBasedSorter" strictSorting="true" expectDistanceOne="false">
  <Distinguishing>
   <Tag name="SeriesInstanceUID" group="0020" element="000e"/>
   <Tag name="StudyInstanceUID" group="0020" element="000d"/>
   <Tag name="PatientID" group="0010" element="0020"/>
   <Tag name="PatientName" group="0010" element="0010"/>
  </Distinguishing>
  <Sorting>
   <Tag name="SliceLocation" group="0x0020" element="0x1041"/>
  </Sorting>
</DICOMDatasetSorter>
</DICOMFileReader>
