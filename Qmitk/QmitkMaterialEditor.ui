    <connection>
        <sender>m_PbOK</sender>
        <signal>clicked()</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnOKClicked()</slot>
    </connection>
    <connection>
        <sender>m_PbCancel</sender>
        <signal>clicked()</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnCancelClicked()</slot>
    </connection>
    <connection>
        <sender>m_SlColor</sender>
        <signal>valueChanged(int)</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnColorChanged(int)</slot>
    </connection>
    <connection>
        <sender>m_SlSpecularPower</sender>
        <signal>valueChanged(int)</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnSpecularPowerChanged(int)</slot>
    </connection>
    <connection>
        <sender>m_SlOpacity</sender>
        <signal>valueChanged(int)</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnOpacityChanged(int)</slot>
    </connection>
    <connection>
        <sender>m_PbSelectColor</sender>
        <signal>clicked()</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnColorSelectButtonClickedClicked()</slot>
    </connection>
    <connection>
        <sender>m_SlCoefficients</sender>
        <signal>valueChanged(int)</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnCoefficientsChanged(int)</slot>
    </connection>
    <connection>
        <sender>m_CbInterpolation</sender>
        <signal>activated(int)</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnInterpolationChanged(int)</slot>
    </connection>
    <connection>
        <sender>m_CbRepresentation</sender>
        <signal>activated(int)</signal>
        <receiver>QmitkMaterialEditor</receiver>
        <slot>OnRepresentationChanged(int)</slot>
    </connection>
</connections>
